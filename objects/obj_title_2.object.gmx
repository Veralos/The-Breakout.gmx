        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (rebind) {
    if (rebind_error_time &gt; 0) {
        rebind_error_time--;
    }

    if (rebind_keyboard) {
        if (keyboard_check_pressed(vk_anykey)) {
            if (keyboard_key = vk_escape) {
                audio_play_sound(snd_back, 0, false);
                rebind = false;
            }
            else if(ds_map_exists(obj_input.keys, keyboard_key)) {
                if (!scr_map_value_exists(obj_input.keyboard_controls, keyboard_key)) {
                    audio_play_sound(snd_confirm, 0, false);
                    ds_map_replace(obj_input.keyboard_controls, rebind_control, keyboard_key);
                    scr_open_file();
                    ini_write_real("Keyboard", rebind_control, keyboard_key);
                    ini_close();
                    rebind = false;
                }
                else {
                    audio_play_sound(snd_error, 0, false);
                    rebind_error = "Key is already in use";
                    rebind_error_time = 60;
                }
            }
            else {
                audio_play_sound(snd_error, 0, false);
                rebind_error = "Key is not supported";
                rebind_error_time = 60;
            }
        }
    }
    else {
        var button = scr_check_gamepad_buttons();
        if (keyboard_check_pressed(vk_escape) || gamepad_button_check_pressed(0, gp_start)) {
            audio_play_sound(snd_back, 0, false);
            rebind = false;
        }
        else if (!is_undefined(button)) {
            if(ds_map_exists(obj_input.buttons, button)) {
                if (!scr_map_value_exists(obj_input.gamepad_controls, button)) {
                    audio_play_sound(snd_confirm, 0, false);
                    ds_map_replace(obj_input.gamepad_controls, rebind_control, button);
                    rebind = false;
                    scr_open_file();
                    ini_write_real("Gamepad", rebind_control, button);
                    ini_close();
                }
                else {
                    audio_play_sound(snd_error, 0, false);
                    rebind_error = "Button is already in use";
                    rebind_error_time = 60;
                }
            }
            else {
                audio_play_sound(snd_error, 0, false);
                rebind_error = "Button is not supported";
                rebind_error_time = 60;
            }
        }
    }
}
else {
    rebind_error_time = 0;

    if (obj_input.select_up_pressed) {
        audio_play_sound(snd_select, 0, false);
        selected--;
    }
    if (obj_input.select_down_pressed) {
        audio_play_sound(snd_select, 0, false);
        selected++;
    }
    
    if (selected &lt; 0) {
        selected = ds_list_size(menu) - 1;
    }
    if (selected &gt; ds_list_size(menu) - 1) {
        selected = 0;
    }
    
    if (obj_input.confirm_pressed) {
        audio_play_sound(snd_confirm, 0, false);
        var selection = ds_list_find_value(menu, selected);
        if (selection[3] == -1) {
            script_execute(selection[1]);
        }
        else {
            script_execute(selection[1], selection[0], selection[3]);
        }
    }
    
    if (obj_input.back_pressed) {
        if (ds_stack_empty(menu_stack)) {
            game_end();
        }
        else {
            scr_back();
        }
    }
}

animate += 0.01;
if (animate &gt; 1) {
    animate = 0;
}
